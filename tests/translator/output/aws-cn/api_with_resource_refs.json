          }, 
          "paths": {
            "/html": {
              "get": {
                "x-amazon-apigateway-integration": {
                  "httpMethod": "POST", 
                  "type": "aws_proxy", 
                  "uri": {
                    "Fn::Sub": "arn:aws-cn:apigateway:${AWS::Region}:lambda:path/2015-03-31/functions/${MyFunction.Arn}/invocations"
                  }
                }, 
                "responses": {}
              }
            }
          }, 
          "swagger": "2.0"
        }, 
        "EndpointConfiguration": {
          "Types": [
            "REGIONAL"
          ]
        }, 
        "Parameters": {
          "endpointConfigurationTypes": "REGIONAL"
        }
      }
